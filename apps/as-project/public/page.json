            {
              "renderer": "input",
              "name": "customer_id",
              "label": "客户UID",
              "placeholder": "占位符",
              "type": "text",
              "validType": "normal",
              "rules": [],
              "requiredOn": "true"
            },
            {
              "renderer": "input",
              "name": "content",
              "label": "博文正文",
              "placeholder": "请填写正文，最多1400个汉字",
              "type": "textarea",
              "maxlength": 1400,
              "showWordLimit": true,
              "validType": "normal",
              "rules": [],
              "requiredOn": "true"
            },
            {
              "renderer": "radio",
              "name": "banner",
              "defaultValue": "1",
              "label": "banner图",
