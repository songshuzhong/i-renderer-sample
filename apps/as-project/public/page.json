              "label": "售卖方式",
              "type": "button",
              "options": [
                {
                  "text": "CPD",
                  "value": "1"
                },
                {
                  "text": "CPM",
                  "value": "2"
                }
              ]
            },
            {
              "renderer": "input",
              "name": "impression_amount",
              "label": "投放量",
              "placeholder": "请填写整数",
              "type": "text",
              "validType": "normal",
              "rules": [],
              "visibleOn": "data.delivery_type==2",
              "requiredOn": "data.delivery_type==2"
            },
            {
              "renderer": "radio",
              "name": "delivery_sort",
              "defaultValue": "2",
              "label": "投放优先级",
              "tip": "",
              "target": "",
              "type": "button",
              "options": [
                {
                  "text": "首刷",
                  "value": "1"
                },
                {
                  "text": "二刷",
                  "value": "2"
                }
              ]
            },
            {
              "renderer": "radio",
              "name": "special_style",
              "defaultValue": "3",
              "label": "互动类型",
              "type": "button",
              "options": [
                {
                  "text": "无",
                  "value": "0"
                },
                {
                  "text": "横滑",
                  "value": "1"
                },
                {
                  "text": "长按",
                  "value": "2",
