              "type": "text",
              "validType": "normal",
              "rules": [],
              "visibleOn": "data.video_while_watching"
            },
            {
              "renderer": "switch",
              "name": "frame_out_loop_flag",
              "defaultValue": false,
              "label": "结尾循环",
              "width": 40,
              "visibleOn": "data.special_style == 3"
            },
            {
              "renderer": "input",
              "name": "frame_out_loop_start_time",
              "defaultValue": "2",
              "label": "循环开始时间",
              "placeholder": "占位符",
              "type": "text",
              "validType": "normal",
              "rules": [
                {
                  "exp": "data.duration && data.frame_out_loop_start_time > data.duration",
                  "message": "超过视频长度",
                  "trigger": "blur"
                },
                {
                  "exp": "data.frame_out_loop_start_time<1",
                  "message": "开始时间大于0",
                  "trigger": "blur"
                }
              ],
              "visibleOn": "data.special_style == 3"
            },
            {
              "renderer": "radio",
              "name": "frame_out_action_type",
              "defaultValue": "2",
              "label": "出框动作结果",
              "tip": "2",
