              "options": [
                {
                  "text": "普通链接",
                  "value": "2"
                },
                {
                  "text": "直达链接",
                  "value": "3"
                },
                {
                  "text": "同步视频播放页H5",
                  "value": "1",
                  "disabledOn": "!data.video_while_watching"
                }
              ],
              "visibleOn": "data.special_style == 3"
            },
            {
              "renderer": "input",
              "name": "frame_out_direct_link",
              "label": "直达链接",
              "placeholder": "请填直达链接",
              "type": "text",
              "validType": "normal",
              "rules": [],
              "visibleOn": "data.frame_out_action_type == 3",
              "requiredOn": "data.frame_out_action_type == 3"
