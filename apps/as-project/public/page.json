              "actionUrl": "https://www.fastmock.site/mock/a93e0b29161761b8153cbc02db04c643/api/upload",
              "listType": "picture-card",
              "limit": 1,
              "feedbackConfig": [],
              "visibleOn": "data.banner == 1",
              "requiredOn": "true"
            },
            {
              "renderer": "combo",
              "name": "images",
              "label": "图片集合",
              "type": "array",
              "length": 2,
              "visibleOn": "data.banner == 4",
              "controls": [
                {
                  "renderer": "upload",
                  "name": "upload",
                  "label": "",
                  "preview": true,
                  "validType": "normal",
                  "autoUpload": true,
                  "actionUrl": "https://www.fastmock.site/mock/a93e0b29161761b8153cbc02db04c643/api/upload",
                  "listType": "picture",
                  "limit": 1,
                  "accept": ".mp4,.txt",
                  "showFileList": false,
                  "feedbackConfig": [],
                  "requiredOn": "true"
                }
              ],
              "rules": [
                {
                  "exp": "data.images.length<2",
                  "message": "图片集合长度是2",
                  "trigger": "blur"
                }
              ]
            },
            {
              "renderer": "upload",
              "visibleOn": "data.special_style == 3",
              "name": "video_info",
              "label": "出框彩色视频",
              "preview": true,
              "validType": "normal",
              "type": "video",
              "listType": "picture-card",
              "feedbackConfig": [
                "fid",
                "duration",
                "url",
                "width",
                "height",
                "src",
                "cover"
              ],
              "rules": [
                {
                  "required": false
                }
              ],
              "autoUpload": true,
              "actionUrl": " https://mock.mengxuegu.com/mock/6711b0089872121dfd71453f/api/upload?type=video",
              "limit": 1,
              "accept": ".mp4",
              "spliced": false,
              "astrict": {
                "maxByte": "10000"
              },
              "data": {},
              "requiredOn": "true"
            },
            {
              "renderer": "switch",
              "name": "video_while_watching",
              "defaultValue": false,
              "label": "视频边看边系列",
              "tip": "",
              "width": 40
            },
            {
              "renderer": "input",
              "name": "video_h5_link",
