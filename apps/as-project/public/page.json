            {
              "renderer": "input",
              "name": "frame_out_landing_page",
              "label": "落地页链接",
              "placeholder": "占位符",
              "type": "text",
              "validType": "normal",
              "rules": [],
              "visibleOn": "data.frame_out_action_type == 2",
              "requiredOn": "data.frame_out_action_type == 2"
            },
            {
              "renderer": "switch",
              "name": "allow_comment",
              "defaultValue": false,
              "label": "开启评论",
              "tip": "",
              "width": 40
            },
            {
              "renderer": "switch",
              "name": "is_private",
              "defaultValue": false,
              "label": "设为私密",
              "tip": "",
              "width": 40
            },
            {
              "renderer": "computed",
              "visibleOn": "$.video_info",
              "name": "frame_out_alpha_video",
              "exp": "data.fid",
              "deps": [
                "fid"
              ]
            },
            {
              "renderer": "computed",
              "visibleOn": "$.video_info",
              "name": "fid",
              "exp": "data.video_info.fid",
              "deps": [
                "video_info"
              ]
            },
            {
              "renderer": "computed",
              "visibleOn": "$.video_info",
              "name": "video_src",
              "exp": "data.video_info.src",
              "deps": [
                "video_info"
              ]
            },
            {
              "renderer": "computed",
              "visibleOn": "$.video_info",
              "name": "video_cover",
              "exp": "data.video_info.cover",
              "deps": [
                "video_info"
              ]
            },
            {
              "renderer": "computed",
              "name": "frame_out_action_type",
              "exp": "$.frame_out_action_type == '1' && '2'",
              "deps": ["video_while_watching"]
            },
